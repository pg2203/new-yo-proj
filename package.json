  "scripts": {
    "check-coverage": "node ./node_modules/istanbul/lib/cli.js check-coverage --statement 75 --branch 60 --function 65",
    "check-dependencies": "check-dependencies --verbose --package-manager bower --scope-list dependencies",
    "check-dev-dependencies": "check-dependencies --verbose --package-manager bower --scope-list devDependencies",
    "check-security": "npm install -g nsp && nsp check",
    "check-updates": " npm install -g npm-check-updates && ncu",
    "eslint": "eslint ./app ./test ./e2e --fix --cache",
    "install": "node ./node_modules/protractor/bin/webdriver-manager update",
    "jscs": "jscs ./app/scripts --fix",
    "jshint": "grunt jshint",
    "lint": "npm run eslint && npm run jscs",
    "postinstall": "node ./node_modules/selenium-standalone/bin/selenium-standalone install",
    "scsslint": "scss-lint -c .scss-lint.yml",
    "test": "grunt test",
    "test-e2e": "protractor protractor.ci.conf.js",
    "validate": "npm ls"
  },
  "pre-commit": [
    "eslint",
    "jscs",
    "test"
